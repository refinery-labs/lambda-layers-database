{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "lambda-layers-ssr-server",
      "memory": 1024,
      "image": "134071937287.dkr.ecr.us-west-2.amazonaws.com/lambda-layers-database",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 8000
        }
      ],
      "links": [
        "lambda-layers-api-server"
      ],
      "logConfiguration": {
      	"logDriver": "awslogs",
        "options": {
          "awslogs-group": "EB-Lambda-Layers-Database-Container-Output",
          "awslogs-region": "us-west-2"
        }
      }
    },
    {
      "name": "lambda-layers-api-server",
      "memory": 768,
      "image": "134071937287.dkr.ecr.us-west-2.amazonaws.com/lambda-layers-database-back-end",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 7777,
          "containerPort": 7777
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "EB-Lambda-Layers-Database-API-Container-Output",
          "awslogs-region": "us-west-2"
        }
      }
    }
  ]
}
